{
  "extends": [
    "@rocketseat/eslint-config/react",
    "plugin:import/typescript"
  ],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off"
  },
  "plugins": ["import"],
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": "tsconfig.json"
      }
    }
  }
}